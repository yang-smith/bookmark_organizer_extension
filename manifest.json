{
    "manifest_version": 3,
    "name": "BookMark Organizer",
    "version": "0.1.1",
    "description": "一键帮助整理您的Chrome收藏夹",
    "permissions": [
      "bookmarks"
    ],
    "default_locale": "en",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo128.png"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
}
  